{
  "sessionExpiration": 3600000,
  "sessionCookieName": "sessionId",
  "sessionCookie": {
    "name": "sessionId",
    "options": {
      "path": "/",
      "httpOnly": true,
      "secure": true,
      "__comment": "Cookie with SECURE attibute will work just if SSL is enabled, if you wants to use the server without encryption you should consider to set it to false"
    }
  },
  "caseSensitiveRoutes": true,
  "timeout": 15000,
  "timeoutPurchaseItemPGW": 30000,
  "log": {
    "trasport": "file",
    "level": "debug",
    "filename": "logs/agl.log",
    "_comment": "trasport can be console or file | filename is ignored for console | In case of OCP env the trasport will be ignored and set every time to console by default"
  },
  "morganLogger": {
    "dateFormat": "YYYY-MM-DD",
    "monitoringLogFormat": "ts: :customDate | logLevel: MONITORING | appId: AGL | thread: :processPid | clientIp: :clientIp | SID: :sessionId | TN: :transactionId | method: :method | apiType: NANO | userId: :userId | deviceId: :deviceId | api: :url | response: resultCode=:resultCode, errorDescription=:errorDescription | platform: :channel | executionTime(ms): :response-time[0]",
    "analyticsLogFormat": "ts: :customDate | logLevel: ANALYTICS | appId: AGL | thread: :processPid | clientIp: :clientIp | SID: :sessionId | TN: :transactionId | :track | :device | :app | :customer | :nav | :custom | apiType: NANO | userId: :userId | api: :url | platform: :channel",
    "directory": "logs/",
    "monitoringLogFile": "mon-%DATE%.log",
    "monitoringLogEnabled": false,
    "analyticsLogFile": "analytics-%DATE%.log",
    "analyticsLogEnabled": false,
    "rotationFrequency": "24h",
    "morganTokens": [
      {
        "key": "customDate",
        "value": "new Date(new Date().getTime() - (new Date().getTimezoneOffset() * 60 * 1000)).toISOString().replace('T', ' ').replace('Z', '');"
      },
      {
        "key": "clientIp",
        "value": "req.transaction.ip"
      },
      {
        "key": "sessionId",
        "value": "req.transaction.clientId"
      },
      {
        "key": "transactionId",
        "value": "req.transaction.id"
      },
      {
        "key": "crmAccount",
        "value": "req.cookies.crm_account"
      },
      {
        "key": "track",
        "value": "req.headers['x-ch-track']"
      },
      {
        "key": "device",
        "value": "req.headers['x-ch-device']"
      },
      {
        "key": "app",
        "value": "req.headers['x-ch-app']"
      },
      {
        "key": "customer",
        "value": "req.headers['x-ch-customer']"
      },
      {
        "key": "nav",
        "value": "req.headers['x-ch-nav']"
      },
      {
        "key": "custom",
        "value": "req.headers['x-ch-custom']"
      },
      {
        "key": "resultCode",
        "value": "res.locals.fullResult.resultCode"
      },
      {
        "key": "errorDescription",
        "value": "res.locals.fullResult.errorDescription"
      },
      {
        "key": "processPid",
        "value": "process.pid"
      },
      {
        "key": "channel",
        "value": "req.transaction.channel"
      },
      {
        "key": "userId",
        "value": "req.transaction.avsToken ? req.transaction.avsToken.userId : '-'"
      },
      {
        "key": "deviceId",
        "value": "(req.transaction.avsToken && req.transaction.avsToken.deviceInfo) ? req.transaction.avsToken.deviceInfo.deviceId : '-'"
      }
    ]
  },
